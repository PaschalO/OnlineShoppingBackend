{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node-dev src/index.js",
    "test": "set ENV=test db-migrate up --config database.json -e test && jasmine-ts && db-migrate db:drop test",
    "tsc": "tsc",
    "db": "nodemon src/database.ts",
    "serve": "nodemon src/index.ts",
    "migrate": "db-migrate up && db-migrate up",
    "build": "npx tsc",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build --onSuccess 'node ./build/index.js'"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.1.3",
    "db-migrate-pg": "^1.3.0",
    "express": "^4.17.1",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.10.10",
    "@types/pg": "^7.14.11",
    "dotenv": "^16.1.4",
    "jasmine": "^4.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^1.14.9",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4"
  }
}
